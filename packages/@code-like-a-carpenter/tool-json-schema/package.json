{
  "name": "@code-like-a-carpenter/tool-json-schema",
  "description": "Generates TypeScript definitions from JSON Schema files",
  "author": "Ian Remmel <1182361+ianwremmel@users.noreply.github.com> (https://www.ianwremmel.com)",
  "bugs": "https://www.github.com/code-like-a-carpenter/workbench/issues",
  "engines": {
    "node": "20.x"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/types/index.d.ts",
        "carpentry": "./src/index.ts",
        "default": "./dist/esm/index.mjs"
      },
      "require": {
        "types": "./dist/types/index.d.ts",
        "carpentry": "./src/index.ts",
        "default": "./dist/cjs/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "homepage": "https://www.github.com/code-like-a-carpenter/workbench/tree/main/packages/@code-like-a-carpenter/tool-json-schema",
  "license": "MIT",
  "repository": "git@github.com:code-like-a-carpenter/workbench.git",
  "publishConfig": {
    "access": "public"
  },
  "executors": "./executors.json",
  "dependencies": {
    "@code-like-a-carpenter/cli-core": "*",
    "@code-like-a-carpenter/tooling-common": "*",
    "@nx/devkit": "^19.1.1",
    "glob": "^10.4.1",
    "json-schema-to-typescript": "^14.0.4"
  }
}
