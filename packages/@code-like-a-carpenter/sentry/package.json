{
  "name": "@code-like-a-carpenter/sentry",
  "description": "Separates Sentry out from the main telemetry package",
  "author": "Ian Remmel <1182361+ianwremmel@users.noreply.github.com> (https://www.ianwremmel.com)",
  "bugs": "https://www.github.com/code-like-a-carpenter/workbench/issues",
  "engines": {
    "node": "22.x"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/esm/index.mjs"
      },
      "require": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/cjs/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "homepage": "https://www.github.com/code-like-a-carpenter/workbench/tree/main/packages/@code-like-a-carpenter/sentry",
  "license": "MIT",
  "repository": "git@github.com:code-like-a-carpenter/workbench.git",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@code-like-a-carpenter/env": "*",
    "@code-like-a-carpenter/errors": "*",
    "@code-like-a-carpenter/telemetry": "*",
    "@sentry/aws-serverless": "^8.30.0"
  },
  "releaseAll": 5,
  "types": "./dist/types/index.d.ts",
  "type": "module"
}
