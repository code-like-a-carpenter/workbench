{
  "name": "@code-like-a-carpenter/tool-inliner",
  "description": "Inlines a text file as a string in a typescript file",
  "author": "Ian Remmel <1182361+ianwremmel@users.noreply.github.com> (https://www.ianwremmel.com)",
  "bugs": "https://www.github.com/code-like-a-carpenter/workbench/issues",
  "engines": {
    "node": "20.x"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/types/index.d.ts",
        "carpentry": "./src/index.ts",
        "default": "./dist/esm/index.mjs"
      },
      "require": {
        "types": "./dist/types/index.d.ts",
        "carpentry": "./src/index.ts",
        "default": "./dist/cjs/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "homepage": "https://www.github.com/code-like-a-carpenter/workbench/tree/main/packages/@code-like-a-carpenter/tool-inliner",
  "license": "MIT",
  "repository": "git@github.com:code-like-a-carpenter/workbench.git",
  "publishConfig": {
    "access": "public"
  },
  "executors": "./executors.json",
  "dependencies": {
    "@babel/generator": "^7.24.6",
    "@babel/template": "^7.24.6",
    "@babel/types": "^7.24.6",
    "@code-like-a-carpenter/assert": "*",
    "@code-like-a-carpenter/cli-core": "*",
    "@nx/devkit": "^19.1.1"
  },
  "types": "./dist/types/index.d.ts",
  "type": "module"
}
