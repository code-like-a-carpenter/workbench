{
  "name": "@code-like-a-carpenter/dotenv",
  "description": "Wrapper around dotenv the loads the right .env file(s) based on the NODE_ENV",
  "author": "Ian Remmel <1182361+ianwremmel@users.noreply.github.com> (https://www.ianwremmel.com)",
  "bugs": "https://www.github.com/code-like-a-carpenter/workbench/issues",
  "engines": {
    "node": "20.x"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/types/index.d.ts",
        "carpentry": "./src/index.ts",
        "default": "./dist/esm/index.mjs"
      },
      "require": {
        "types": "./dist/types/index.d.ts",
        "carpentry": "./src/index.ts",
        "default": "./dist/cjs/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "homepage": "https://www.github.com/code-like-a-carpenter/workbench/tree/main/packages/@code-like-a-carpenter/dotenv",
  "license": "MIT",
  "repository": "git@github.com:code-like-a-carpenter/workbench.git",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "dotenv": "^16.4.5"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0"
  },
  "releaseAll": 5
}
