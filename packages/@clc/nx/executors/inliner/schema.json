{
  "$schema": "http://json-schema.org/schema",
  "version": 2,
  "title": "Inliner executor",
  "description": "",
  "type": "object",
  "properties": {
    "exportName": {
      "type": "string",
      "description": "The name of the export to inline"
    },
    "sourceFile": {
      "type": "string",
      "description": "The file to inline"
    },
    "targetFile": {
      "type": "string",
      "description": "The file to write the inlined content to"
    }
  },
  "required": ["exportName", "sourceFile", "targetFile"],
  "additionalProperties": false
}
