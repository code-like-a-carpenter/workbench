"""
A summary of all accounts.
"""
type Metric implements Model & Timestamped & Versioned
  @model
  @primaryKey(
    pkFields: []
    pkPrefix: "BUSINESS_METRIC"
    skFields: ["onFreeTrial"]
    skPrefix: "SUMMARY"
  ) {
  createdAt: Date!
  id: ID!
  updatedAt: Date!
  version: Int!

  count: Int!
  onFreeTrial: Boolean!
  monthlyRecurringRevenueInCents: Int!
}
